{
    "name": "michaelthedev/superban",
    "description": "Laravel package to ban clients completely for a period of time",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Michaelthedev\\Superban\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Michael A.",
            "email": "michael@logad.net"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.37"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    }
}
