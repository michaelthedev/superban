{
    "name": "michaelthedev/superban",
    "description": "Laravel package to ban clients completely for a period of time",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Michaelthedev\\Superban\\": "/src",
            "Michaelthedev\\Tests\\": "/tests"
        }
    },
    "authors": [
        {
            "name": "Michael A.",
            "email": "michael@logad.net"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Michaelthedev\\Superban\\SuperbanServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.37"
    },
    "require-dev": {
        "pestphp/pest": "^2.28",
        "orchestra/testbench": "^8.18",
        "pestphp/pest-plugin-laravel": "^2.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
